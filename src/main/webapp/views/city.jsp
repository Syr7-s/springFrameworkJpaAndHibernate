<body>


	<div class="container-contact100" style="background-color: black;">


		<div class="wrap-contact100">
			<div class="contact100-form-title" style="background-color:chocolate">
			
			</div>

			<form class="contact100-form validate-form">
				<div class="wrap-input100 validate-input" data-validate="Name is required">
					<span class="label-input100">City Name:</span>
					<input class="input100" type="text" name="countryName" placeholder="City Name">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input">
					<span class="label-input100">City Country Code:</span>
					<input class="input100" type="text" name="countryCode" placeholder="Country Code">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input">
					<span class="label-input100">City District:</span>
					<input class="input100" type="text" name="district" placeholder="District">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100 validate-input" >
					<span class="label-input100">City Population:</span>
					<input class="input100" type="number" name="population" placeholder="Population">
					<span class="focus-input100"></span>
				</div>

			

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						<span>
							Submit
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
			</form>
		</div>
	</div>



	<div id="dropDownSelect1"></div>

